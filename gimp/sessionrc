            (dockable "gimp-undo-history"
                (tab-style icon)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-navigation-view"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-sample-point-editor"
                (tab-style automatic))
            (dockable "gimp-vectors-list"
                (tab-style automatic)
                (preview-size 12)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-device-status"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))))
