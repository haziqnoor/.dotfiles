# Aliases for all shells
alias which-shell='ps -p $$'
alias ..='cd ..'
alias ...='cd ../../'
alias ....='cd ../../../'
alias pid='ps -cvx | grep'
alias ls='ls --color=always'
alias grep='grep --color=always'
alias less='less -R'
alias vi='/usr/bin/vi'
alias copyc='xclip -selection clipboard'
alias copyp='xclip -selection primary'
alias pastec='xclip -selection clipboard -o'
alias pastep='xclip -selection primary -o'
alias clock='watch -n 1 -p -t date +%r'
alias ink='ink -p usb'
alias mstash='encfs ~/.stash ~/docs/stash'
alias ustash='fusermount -u ~/docs/stash'
alias tor_off='source tor_off'
alias tor_on='source tor_on'
alias cal='rem -m -b1 -c+2'
alias cower='cower -calways'
alias whois='whois -H'
alias flash='notify youtube-dl'
alias youtube='notify youtube-dl -f 18 --youtube-skip-dash-manifest'
alias vimeo='notify youtube-dl -f http-360p'
alias anki='anki -b /home/haziq/.anki/'
alias terrainsculptor='java -jar /home/haziq/src/OSUCartography/TerrainSculptor.jar'
alias screepainter='java -jar /home/haziq/src/OSUCartography/ScreePainter.jar'
alias pyramidshader='java -jar /home/haziq/src/OSUCartography/PyramidShader.jar'
